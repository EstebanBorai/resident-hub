{
  "name": "fastify-next-project",
  "version": "0.1.0",
  "main": "server.ts",
  "repository": "https://github.com/EstebanBorai/fastify-next-project.git",
  "author": "Esteban Borai <estebanborai@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "NEXT_TELEMETRY_DISABLED=1 yarn build:client && yarn build:server",
    "build:production": "yarn run build && ./bin/prepare_prod",
    "build:client": "NEXT_TELEMETRY_DISABLED=1 next build ./src/client",
    "build:server": "tsc",
    "dev": "NODE_ENV=development NEXT_TELEMETRY_DISABLED=1 ts-node-dev --ignore-watch ./src/client --transpile-only ./src/main.ts",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "fmt": "eslint . --ext .js,.jsx,.ts,.tsx --fix && prettier --write \"**/*.{json,md,yml,ts,tsx,js,jsx}\"",
    "knex": "env-cmd knex --knexfile ./src/config/knexfile.ts",
    "knex:migrate:latest": "env-cmd knex --knexfile ./src/config/knexfile.ts migrate:latest",
    "prettier": "prettier -c \"**/*.{md,json,yml,sh}\"",
    "start": "NEXT_TELEMETRY_DISABLED=1 node ./dist/main.js",
    "test": "jest"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/fastify-cors": "^2.1.0",
    "@types/fastify-jwt": "^0.8.1",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/knex": "^0.16.1",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "autoprefixer": "^10.2.5",
    "dotenv": "^9.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "postcss": "^8.2.15",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.5",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "fastify": "^3.15.1",
    "fastify-cookie": "^5.3.0",
    "fastify-cors": "^6.0.1",
    "fastify-jwt": "^2.4.0",
    "fastify-plugin": "^3.0.0",
    "formik": "^2.2.6",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.6",
    "knex-stringcase": "^1.4.5",
    "mongoose": "^5.12.7",
    "next": "^10.2.0",
    "pg": "^8.6.0",
    "pino-pretty": "^4.7.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
